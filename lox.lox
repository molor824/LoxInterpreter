var i = 1;
var fizz = 3;
var buzz = 5;
var n = 0x10000;

while (i <= n) {
    var divisable = false;

    if (i % fizz == 0) {
        print("fizz");
        divisable = true;
    }
    if (i % buzz == 0) {
        print("buzz");
        divisable = true;
    }
    if (!divisable) {
        print(i);
    }

    i++;
}